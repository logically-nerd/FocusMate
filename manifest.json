{
  "name": "FocusMate",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Your all-in-one productivity tool.",
  "action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "services/background.js"
  },
  "permissions": ["storage"]
}
